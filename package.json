{
  "name": "hackathon-starter",
  "version": "4.3.0",
  "description": "A boilerplate for Node.js web applications",
  "repository": {
    "type": "git",
    "url": "https://github.com/sahat/hackathon-starter.git"
  },
  "author": "Sahat Yalkabov",
  "license": "MIT",
  "scripts": {
    "start": "node app.js",
    "test": "mocha --reporter spec",
    "build": "browserify -t [ babelify --presets [ react es2015 ] ] src/index.jsx -o public/js/app.js",
    "watch": "watchify src/index.jsx -t [ babelify --presets [ es2015 react ] ] -o public/js/app.js",
    "devstart": "nodemon app.js",
    "dev": "npm run devstart | npm run watch",
    "webpack-server": "webpack-dev-server --content-base public/ --config webpack.dev.config.js --history-api-fallback --hot --inline --host 0.0.0.0"
  },
  "dependencies": {
    "async": "^2.1.2",
    "axios": "^0.15.3",
    "babel-loader": "^6.3.2",
    "babelify": "^7.3.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.15.2",
    "browserify": "^14.1.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "classnames": "^2.2.5",
    "clockwork": "^0.1.4",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.14.2",
    "express-status-monitor": "^0.1.5",
    "express-validator": "^3.1.2",
    "fbgraph": "^1.3.0",
    "file-loader": "^0.10.0",
    "flexboxgrid": "^6.3.1",
    "github": "^7.2.0",
    "html-webpack-plugin": "^2.28.0",
    "instagram-node": "^0.5.8",
    "lastfm": "^0.9.2",
    "less-loader": "^2.2.3",
    "lob": "^3.9.0",
    "lodash": "^4.16.6",
    "lusca": "^1.4.1",
    "material-ui": "^0.16.4",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-foursquare": "^0.3.0",
    "node-linkedin": "^0.5.4",
    "node-sass-middleware": "^0.10.0",
    "nodemailer": "^2.6.4",
    "nodemon": "^1.11.0",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-linkedin-oauth2": "^1.4.1",
    "passport-local": "^1.0.0",
    "passport-oauth": "^1.0.0",
    "passport-openid": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "paypal-rest-sdk": "^1.7.0",
    "postcss-loader": "^1.3.1",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.1",
    "react-dom": "^15.4.2",
    "react-flexbox-grid": "^0.10.2",
    "react-hot-loader": "^1.3.1",
    "react-tap-event-plugin": "^2.0.1",
    "request": "^2.78.0",
    "stripe": "^4.12.0",
    "tumblr.js": "^1.1.1",
    "twilio": "^3.3.1-edge",
    "twit": "^2.2.5",
    "validator": "^6.1.0",
    "watchify": "^3.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^3.1.2",
    "npm": "^4.2.0",
    "sinon": "^1.17.6",
    "sinon-mongoose": "^1.3.0",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    }
  }
}
